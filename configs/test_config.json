{
  "pipeline_info": {
    "name": "Eagle Ford 5-Well Test Configuration",
    "description": "Optimized config for 5 LAS files with complete analysis including validation splits, plots, and SHAP",
    "version": "1.0",
    "created_date": "2025-11-26",
    "target_platform": ["local", "vscode", "mac_m2"],
    "expected_dataset_size": "small"
  },
  
  "global": {
    "run_mode": "test",
    "target_column": "GR",
    "formation": "eagle_ford",
    "random_state": 42,
    "n_jobs": 4,
    "max_memory_usage": 0.7,
    "output_compression": null,
    "enable_gpu": false,
    "chunk_processing": false,
    "save_intermediate_files": true,
    "detailed_logging": true,
    "enable_caching": false,
    "fast_execution": true
  },
  
  "logging": {
    "level": "DEBUG",
    "enable_progress_bars": true,
    "log_to_file": true,
    "log_to_console": true,
    "detailed_timing": true,
    "memory_monitoring": true,
    "save_debug_info": true,
    "log_model_metrics": true,
    "log_feature_importance": true,
    "verbose_validation": true
  },
  
  "preprocessing": {
    "input_pattern": "*.las",
    "target_step_size": 1.0,
    "quality_threshold": 0.3,
    "batch_size": 10,
    "completeness_threshold": 85.0,
    "imputation_threshold": 30.0,
    "outlier_mad_threshold": 4.0,
    "gr_constraints": {
      "min": 10.0,
      "max": 250.0
    },
    "outlier_detection": true,
    "outlier_method": "iqr",
    "outlier_threshold": 2.5,
    "geological_constraints": true,
    "imputation_method": "knn",
    "knn_neighbors": 3,
    "interpolation_method": "linear",
    "max_missing_percentage": 30.0,
    "chunk_processing": false,
    "chunk_size": 5000,
    "temp_file_cleanup": false,
    "save_processing_report": true,
    "save_well_statistics": true,
    "save_quality_metrics": true
  },
  
  "feature_engineering": {
    "rolling_windows": [3, 5, 10],
    "sequence_length": 25,
    "target_curves": ["GR"],
    "geological_features": true,
    "cross_curve_features": true,
    "depth_normalization": "minmax",
    "feature_expansion_ratio": 8,
    "gradient_orders": [1, 2],
    "polynomial_degree": 2,
    "enable_cross_features": true,
    "max_cross_features": 200,
    "depth_features": true,
    "formation_windows": [[8000, 12000]],
    "lithology_indicators": true,
    "completion_features": true,
    "correlation_threshold": 0.95,
    "variance_threshold": 0.01,
    "feature_importance_selection": true,
    "max_features": 300,
    "test_size": 0.2,
    "validation_size": 0.2,
    "stratify_by_operator": false,
    "min_wells_per_split": 1,
    "scaling_method": "standard",
    "target_transformation": null,
    "save_scalers": true,
    "save_feature_importance": true,
    "save_feature_statistics": true,
    "save_correlation_matrix": true
  },
  
  "machine_learning": {
    "models": ["random_forest", "xgboost"],
    "primary_metric": "r2",
    "cv_folds": 2,
    "search_method": "random",
    "normalization": null,
    "outlier_threshold": 3.0,
    "optimization_method": "random",
    "max_optimization_time": 180,
    "n_iter_search": 2,
    "early_stopping": true,
    "early_stopping_patience": 5,
    
    "random_forest": {
      "n_estimators_range": [10, 50],
      "max_depth_range": [3, 8],
      "min_samples_split_range": [2, 5],
      "min_samples_leaf_range": [1, 3],
      "max_features": ["sqrt", "log2"],
      "bootstrap": true,
      "oob_score": true,
      "n_jobs": 4
    },
    
    "xgboost": {
      "n_estimators_range": [10, 50],
      "max_depth_range": [3, 6],
      "learning_rate_range": [0.05, 0.2],
      "subsample_range": [0.8, 1.0],
      "colsample_bytree_range": [0.8, 1.0],
      "reg_alpha_range": [0, 0.5],
      "reg_lambda_range": [0, 0.5],
      "min_child_weight_range": [1, 3],
      "gamma_range": [0, 0.2],
      "objective": "reg:squarederror",
      "eval_metric": "rmse",
      "tree_method": "hist",
      "n_jobs": 4
    },
    
    "feature_selection_method": "none",
    "dimensionality_reduction": null,
    "handle_class_imbalance": false,
    "outlier_detection_ml": true,
    "outlier_contamination": 0.1,
    "enable_shap_analysis": true,
    "shap_sample_size": 1000,
    "feature_importance_plots": true,
    "learning_curves": true,
    "validation_curves": true,
    "save_models": true,
    "save_predictions": true,
    "model_format": "joblib",
    "compress_models": false,
    "save_evaluation_metrics": true,
    "save_hyperparameter_results": true
  },
  
  "paths": {
    "input_dir": "dataset/test",
    "output_dir": "test_output",
    "logs_dir": "test_output/logs",
    "preprocessing_dir": "test_output/preprocessing",
    "features_dir": "test_output/features", 
    "models_dir": "test_output/models",
    "reports_dir": "test_output/reports"
  },
  
  "test_specific": {
    "fast_execution": true,
    "skip_heavy_computations": false,
    "minimal_feature_set": false,
    "debug_mode": true,
    "save_all_intermediates": true,
    "generate_plots": true,
    "validate_outputs": true,
    "comprehensive_testing": true,
    "validate_pipeline_integrity": true
  },
  
  "validation": {
    "validate_each_stage": true,
    "check_data_quality": true,
    "verify_feature_creation": true,
    "test_model_performance": true,
    "generate_validation_report": true,
    "save_intermediate_validations": true
  }
}